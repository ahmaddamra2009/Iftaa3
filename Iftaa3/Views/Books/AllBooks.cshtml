@model IEnumerable<Book>
@{
    ViewData["Title"] = "All Books";
}

<h1>All Books</h1>

<table class="table table-bordered">
    <tr>
        <td>@Html.DisplayNameFor(model => model.BookName)</td>
        <td>@Html.DisplayNameFor(model => model.Author.AuthorName)</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.BookName</td>
            <td>@item.Author.AuthorName</td>
        </tr>

    }
</table>

